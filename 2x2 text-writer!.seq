LOAD     = $AASAVE     = LOAD+2TITLE    = $4100TLINE    = 1024+(40*14);---------------------------------------WRITE2X2         LDX #<TITLE         LDY #>TITLE         STX LOAD         STY LOAD+1         LDX #<TLINE         LDY #>TLINE         STX SAVE         STY SAVE+1         LDA #$00         STA COUNTLOOP2X2  LDY COUNT         LDA (LOAD),Y         LDY #$00         STA (SAVE),Y         CLC         ADC #64         INY         STA (SAVE),Y         CLC         ADC #64         LDY #40         STA (SAVE),Y         CLC         ADC #64         INY         STA (SAVE),Y         LDA SAVE         CLC         ADC #$02         STA SAVE         LDA SAVE+1         ADC #$00         STA SAVE+1         INC COUNT         LDA COUNT         CMP #20         BNE LOOP2X2         RTS;---------------------------------------