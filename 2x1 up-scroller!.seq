;---------------------------------------       ; ÉN ÉRQ.       ; --------       ; LDA YPOS       ; ORA #$10       ; STA $D011UDELAY   = *+1UPSCROLL LDA #$00         BEQ UPS1         DEC UDELAY         RTSUPS1     LDA YPOS         SEC         SBC #$01         AND #$07         STA YPOS         BCC MOVEUP         RTS         ;MOVEUP   LDX #$27UPLOOP   LDA $0428,X         STA $0400,X         LDA $0450,X         STA $0428,X         LDA $0478,X         STA $0450,X         LDA $04A0,X         STA $0478,X         LDA $04C8,X         STA $04A0,X         LDA $04F0,X         STA $04C8,X         LDA $0518,X         STA $04F0,X         LDA $0540,X         STA $0518,X         LDA $0568,X         STA $0540,X         LDA $0590,X         STA $0568,X         LDA $05B8,X         STA $0590,X         LDA $05E0,X         STA $05B8,X         LDA $0608,X         STA $05E0,X         LDA $0630,X         STA $0608,X         LDA $0658,X         STA $0630,X         LDA $0680,X         STA $0658,X         DEX         BPL UPLOOP         ;         LDA UPCOUNT         CMP #$01         BEQ UPNEXT0         ;         LDX #$00UPRINT2  LDA $0658,X         CLC         ADC #128         STA $0680,X         INX         CPX #$28         BNE UPRINT2         INC UPCOUNT         JMP UNEXT         ;UPNEXT0  LDX #$00         STX UPCOUNTUTHI     = *+2UTLO     = *+1ULOOP2   LDA UTEXT,X         BNE UPRINT         LDA #$00         STA UTLO         LDA #$28         STA UTHI         LDX #$00         JMP ULOOP2UPRINT   STA $0680,X         INX         CPX #$28         BNE ULOOP2         JMP UPDONE         ;UNEXT    LDA UTLO         CLC         ADC #$28         STA UTLO         BCC UPDONE         INC UTHIUPDONE   RTS;---------------------------------------YPOS     .BYTE $00UPCOUNT  .BYTE $00;---------------------------------------