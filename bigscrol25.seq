;---------------------------------------; ÂIGCHAR ÓCROLLER ÆÕÌÌ ÓCREEN;---------------------------------------APAGE    = $AACPAGE    = APAGE+2TEXT     = $3000CHARSET  = $2000;---------------------------------------         *= $0900         ;         SEI         LDX #$FF         TXS         LDA #$0C         STA $D021         STA 646         JSR $E544         LDA #$00         STA $D020         STA $D021         STA 650         LDA #$80         STA 657         ;         LDA #24         STA $D018         LDA #200         STA $D016         ;         JSR BIGINIT         ;         LDA #$00         JSR $1000         ;         LDA #$7F         STA $DC0D         LDA #$1B         STA $D011         LDA #$5E         STA $D012         LDA #$01         STA $D019         STA $D01A         LDX #<PROGIRQ         LDY #>PROGIRQ         STX $0314         STY $0315         LDX #<RESTORE         LDY #>RESTORE         STX $0318         STY $0319         CLI         ;KEYSCAN  JSR $FFE4         BEQ *-3         JMP KEYSCAN;---------------------------------------RESTORE  ;         JMP $9000         RTI;---------------------------------------PROGIRQ  ;         ;         JSR $1006         ;         LDX #$E8         CPX $D012         BNE *-3         ;         JSR SHIFT1         JSR CMOVER         ;         LDA #$01         STA $D019         JMP $EA31;---------------------------------------BIGSCRL  LDX #$00CMOVE1   LDA $0429,X         STA $0428,X         LDA $04A1,X         STA $04A0,X         LDA $0519,X         STA $0518,X         LDA $0591,X         STA $0590,X         LDA $0609,X         STA $0608,X         LDA $0681,X         STA $0680,X         LDA $06F9,X         STA $06F8,X         LDA $0771,X         STA $0770,X         INX         CPX #$28         BNE CMOVE1         ;         LDA #$20         CLC         ROL CHARD0         BCC DOBIT1         LDA CBYTEDOBIT1   STA $044F         LDA #$20